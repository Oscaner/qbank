{
  "name": "qbank",
  "description": "The framework working for track online question banks",
  "version": "0.0.0",
  "author": "Oscaner",
  "bin": {
    "qbank": "./bin/run.js"
  },
  "bugs": "https://github.com/Oscaner/qbank/issues",
  "dependencies": {
    "@lesjoursfr/html-to-image": "^1",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@resolid/cache-manager-sqlite": "^5",
    "@type-cacheable/cache-manager-adapter": "^17",
    "@type-cacheable/core": "^14",
    "ansi-colors": "^4",
    "async-stream-emitter": "^7",
    "axios": "^1",
    "axios-cache-interceptor": "^1",
    "better-sqlite3": "^11",
    "cache-manager": "^5",
    "chunk": "^0",
    "cli-progress": "^3",
    "data-uri-to-buffer": "^6",
    "form-data": "^4",
    "fs-extra": "^11",
    "html-to-text": "^9",
    "inquirer": "^10",
    "lodash": "^4",
    "md5": "^2",
    "node-html-parser": "^6",
    "object-treeify": "^4",
    "pkg-dir": "^8",
    "playwright": "^1",
    "random-number": "^0",
    "sleep-promise": "^9",
    "streamifier": "^0",
    "tmp": "^0",
    "tty-table": "^4",
    "user-agents": "^1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19",
    "@commitlint/config-conventional": "^19",
    "@commitlint/types": "^19",
    "@oclif/prettier-config": "^0",
    "@oclif/test": "^4",
    "@types/ansi-colors": "^3",
    "@types/async-stream-emitter": "^7",
    "@types/better-sqlite3": "^7",
    "@types/chai": "^4",
    "@types/chunk": "^0",
    "@types/cli-progress": "^3",
    "@types/form-data": "^2",
    "@types/fs-extra": "^11",
    "@types/html-to-text": "^9",
    "@types/lodash": "^4",
    "@types/md5": "^2",
    "@types/mocha": "^10",
    "@types/node": "^22",
    "@types/random-number": "^0",
    "@types/streamifier": "^0",
    "@types/tmp": "^0",
    "@types/user-agents": "^1",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "@typescript-eslint/typescript-estree": "^8",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "eslint-plugin-prettier": "^5",
    "husky": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^3",
    "shx": "^0",
    "ts-node": "^10",
    "typescript": "<5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Oscaner Miao/https://github.com/Oscaner/qbank",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "qbank",
    "dirname": "qbank",
    "commands": "./dist/commands",
    "hooks": {
      "init": [
        "./dist/hooks/init/main"
      ],
      "postrun": [
        "./dist/hooks/postrun/main"
      ]
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "Oscaner Miao/https://github.com/Oscaner/qbank",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky"
  },
  "types": "dist/index.d.ts"
}
